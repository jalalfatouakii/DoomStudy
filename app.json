{
  "expo": {
    "name": "DoomStudy",
    "slug": "doomstudy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/image.png",
    "scheme": "doomstudy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doomstudy.jxlxl",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "appleTeamId": "KH4448LD3D"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.doomstudy.jxlxl"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/books.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#151718",
          "dark": {
            "backgroundColor": "#151718"
          }
        }
      ],
      "@bacons/apple-targets"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d3d22b27-56de-4f65-87b4-a7bf925be0a7",
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "bundleIdentifier": "com.doomstudy.jxlxl.widget",
                  "targetName": "widget",
                  "entitlements": {}
                }
              ]
            }
          }
        }
      }
    }
  }
}
