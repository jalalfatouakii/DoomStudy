{
  "expo": {
    "name": "DoomStudy",
    "slug": "doomstudy",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/image.png",
    "scheme": "doomstudy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doomstudy.jxlxl",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.doomstudy.jxlxl"
        ]
      },
      "appleTeamId": "KH4448LD3D",
      "icon": {
        "light": "./assets/images/ios.png",
        "dark": "./assets/images/darkmode.png"
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.doomstudy.jxlxl",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android.png",
        "backgroundColor": "#151718"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/bg.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#151718",
          "dark": {
            "backgroundColor": "#151718"
          }
        }
      ],
      "@bacons/apple-targets",
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-7857372081627659~5771451405",
          "androidAppId": "ca-app-pub-7857372081627659~3062253933"
        }
      ],
      "@react-native-ai/mlc",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d3d22b27-56de-4f65-87b4-a7bf925be0a7",
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "bundleIdentifier": "com.doomstudy.jxlxl.widget",
                  "targetName": "widget",
                  "entitlements": {}
                }
              ]
            }
          }
        }
      }
    }
  }
}